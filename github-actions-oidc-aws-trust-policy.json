{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "sts:AssumeRoleWithWebIdentity",
            "Principal": {
                "Federated": "arn:aws:iam::053490018989:oidc-provider/token.actions.githubusercontent.com"
            },
            "Condition": {
                "StringEquals": {
                    "token.actions.githubusercontent.com:aud": [
                        "sts.amazonaws.com"
                    ]
                },
                "StringLike": {
                    "token.actions.githubusercontent.com:sub": [
                        "repo:SreeVeerDevOps/DevSecOPsB46-AWSB69-GitHub-Actions:*",
                        "repo:SreeVeerDevOps/DevSecOPsB46-AWSB69-GitHub-Actions-2:*",
                        "repo:SreeVeerDevOps/DevSecOPsB46-AWSB69-GitHub-Actions-3:*"
                    ]
                }
            }
        }
    ]
}